    "vue-loader": "^15.7.1",
    "vue-script2": "^2.1.0",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0",
    "webpack-serve": "^3.1.1"
  },
  "dependencies": {
    "accounting": "0.4.1"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
