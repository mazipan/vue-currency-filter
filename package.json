    "jest": "^24.9.0",
    "jest-serializer-vue": "^2.0.2",
    "lint-staged": "^9.2.5",
    "mini-css-extract-plugin": "^0.8.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^8.0.0",
    "typescript": "^3.6.2",
    "vue": "^2.6.10",
    "vue-google-adsense": "^1.5.0",
    "vue-highlightjs": "1.3.3",
